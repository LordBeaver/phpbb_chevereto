{
    "name": "lordbeaver/chevereto",
    "type": "phpbb-extension",
    "description": "Расширение позволяет автоматически загружать изображения из BB-кодов [img] на хостинг с Chevereto.",
    "homepage": "https://onlystorage.org/page/phpbb",
    "version": "0.4.0",
    "time": "$Date: 2017-05-30 09:09:04 +0300 (Вт, 30 май 2017) $",
    "license": "The 3-Clause BSD License",
    "authors": [
        {
            "name": "Lord Beaver",
            "homepage": "https://onlyfriends.name/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
		"ext-curl": "*"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Chevereto API",
        "soft-require": {
            "phpbb/phpbb": "3.1.*"
        },
        "version-check": {
            "host": "onlystorage.org",
            "directory": "/content/pages",
            "filename": "version.json"
        }
    }
}