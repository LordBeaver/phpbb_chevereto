{
    "name": "lordbeaver/chevereto",
    "type": "phpbb-extension",
    "description": "Расширение позволяет автоматически загружать изображения из BBCode [img] на хостинг с Chevereto.",
    "homepage": "https://onlystorage.org/page/phpbb",
    "version": "0.4.3",
    "time": "2019-05-31",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Lord Beaver",
            "homepage": "https://onlyfriends.name/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Chevereto API",
        "soft-require": {
            "phpbb/phpbb": "3.1.*"
        },
        "version-check": {
            "host": "onlystorage.org",
            "directory": "/content/pages",
            "filename": "version.json"
        }
    }
}